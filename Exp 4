  DIV :C
  CLASS:COMP 1 
  UIN :251P002
  */
  int i,j,start,end,prime;
  printf("enter the first number");
  scanf("%d",&start);
  printf("enter the second number");
  scanf("%d", &end);
  printf("prime number between %d and %d and %d are:",start,end);
  for(i=start; i<=end; i++) {
          if (i<2)
          continue;
          prime = 1;
          for (j = 2; j <= i/2;j++){
              if (i%j==0){
                  prime = 0;
                  break;}
          }
          if (prime)
          printf("%d",i);
      } 
      printf("\n");
      return 0;
              }
